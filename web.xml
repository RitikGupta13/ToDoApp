<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name>first</servlet-name>
        <servlet-class>View.Insert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>first</servlet-name>
        <url-pattern>/web1</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>second</servlet-name>
        <servlet-class>View.Showtasks</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>second</servlet-name>
        <url-pattern>/web2</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>third</servlet-name>
        <servlet-class>View.Status</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>third</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>fourth</servlet-name>
        <servlet-class>View.edit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fourth</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>fifth</servlet-name>
        <servlet-class>View.edit1</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>fifth</servlet-name>
        <url-pattern>/updat</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>sixth</servlet-name>
        <servlet-class>View.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sixth</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
